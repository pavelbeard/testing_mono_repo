{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "lib": [
      "esnext"
    ],
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "@fixtures/*": [
        "./tests/fixtures/*"
      ],
      "@pages/*": [
        "./tests/pages/*"
      ],
      "@helpers/*": [
        "./tests/helpers/*"
      ]
    }
  }
}